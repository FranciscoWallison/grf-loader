{
  "name": "@chicowall/grf-loader",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "version": "1.0.6",
  "description": "A loader for GRF files (Ragnarok Online game file)",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "browser": {
    "./dist/cjs/grf-loader.cjs": "./dist/umd/grf-loader.js"
  },
  "files": [
    "dist/",
    "index.d.ts"
  ],
  "scripts": {
    "test:node": "jest",
    "test:browser": "cypress run",
    "test": "yarn test:node --detectOpenHandles && yarn test:browser",
    "lint": "tsc -p tsconfig.json --noEmit",
    "build": "tsup",
    "all_config": "yarn lint && yarn test:node && yarn test:browser && yarn build"
  },
  "author": "Vincent Thibault <vthibault.mobile@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FranciscoWallison/grf-loader"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FranciscoWallison/grf-loader.git"
  },
  "keywords": [
    "grf-loader",
    "grf-parser",
    "grf-reader",
    "grf",
    "loader",
    "reader",
    "ro",
    "ragnarok",
    "ragnarok-online"
  ],
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@types/cypress": "^1.1.3",
    "@types/jdataview": "^0.0.31",
    "@types/jest": "^25.2.3",
    "@types/node": "^22.15.30",
    "@types/pako": "^1.0.1",
    "babel-jest": "^26.0.1",
    "cypress": "^4.7.0",
    "jest": "^26.0.1",
    "prettier": "2.0.5",
    "tsup": "^8.5.0",
    "typescript": "5"
  },
  "dependencies": {
    "jdataview": "^2.5.0",
    "pako": "^1.0.11"
  },
  "jest": {
    "clearMocks": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "testEnvironment": "node"
  },
  "format": "prettier --write 'src/**/*.ts'"
}