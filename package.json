{
  "name": "@chicowall/grf-loader",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.0.1",
  "description": "A loader for GRF files (Ragnarok Online game file)",
  "main": "./dist/cjs/grf-loader.js",
  "module": "./dist/esm/grf-loader.js",
  "sideEffects": false,
  "browser": {
    "./dist/cjs/grf-loader.js": "./dist/umd/grf-loader.js"
  },
  "files": [
    "dist/",
    "index.d.ts"
  ],
  "scripts": {
    "test:node": "jest",
    "test:browser": "cypress run",
    "test": "yarn test:node --detectOpenHandles && yarn test:browser",
    "lint": "tsc --noEmit",
    "build": "rimraf dist && rollup -c"
  },
  "author": "Vincent Thibault <vthibault.mobile@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FranciscoWallison/grf-loader"
  },
  "repository": "github:FranciscoWallison/grf-loader",
  "keywords": [
    "grf-loader",
    "grf-parser",
    "grf-reader",
    "grf",
    "loader",
    "reader",
    "ro",
    "ragnarok",
    "ragnarok-online"
  ],
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@rollup/plugin-typescript": "^4.1.2",
    "@types/cypress": "^1.1.3",
    "@types/jdataview": "^0.0.31",
    "@types/jest": "^25.2.3",
    "@types/pako": "^1.0.1",
    "babel-jest": "^26.0.1",
    "cypress": "^4.7.0",
    "del-cli": "^3.0.1",
    "jest": "^26.0.1",
    "prettier": "2.0.5",
    "rollup": "^2.12.0",
    "rollup-plugin-terser": "^6.1.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "jdataview": "^2.5.0",
    "pako": "^1.0.11"
  },
  "jest": {
    "clearMocks": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "testEnvironment": "node"
  },
  "format": "prettier --write 'src/**/*.ts'"
}